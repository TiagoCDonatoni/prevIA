{
  "provider": "apifootball",

  "league_source": {
    "mode": "ids",
    "league_ids": [39],
    "max_leagues": 50
  },

  "seasons": {
    "mode": "range",
    "start": 2017,
    "end": 2024
  },

  "endpoints": [
    {
      "id": "teams",
      "path": "/teams",
      "params": { "league": "{league_id}", "season": "{season}" }
    },
    {
      "id": "fixtures",
      "path": "/fixtures",
      "params": { "league": "{league_id}", "season": "{season}" }
    }
  ],

  "paging": {
    "page_param": "page",
    "max_pages_safety": 50
  }
}
